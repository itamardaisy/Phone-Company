@model UI.ClientWebPage.Models.DetailsModel

@{
    ViewBag.Title = "DetailsView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <select id="selectNumber">
        @for (int i = 0; i < Model.ClientLines.Count; i++)
        {
            <option value="@i.ToString()" onclick="">@Model.ClientLines[i]</option>
        }
    </select>
    <script>
        var e = document.getElementById("selectNumber");
        var number = e.options[e.selectedIndex].value;
    </script>
</div>

